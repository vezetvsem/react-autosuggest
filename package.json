    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "chai": "^3.3.0",
    "css-loader": "^0.19.0",
    "eslint": "^1.6.0",
    "eslint-loader": "^1.1.0",
    "eslint-plugin-react": "^3.5.1",
    "extract-text-webpack-plugin": "^0.8.2",
    "jsdom": "<6.5.1",
    "json-loader": "^0.5.3",
    "less": "^2.5.3",
